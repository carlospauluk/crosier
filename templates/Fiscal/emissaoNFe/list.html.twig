{% extends 'list.html.twig' %}


{% set titulo = 'Notas Fiscais' %}
{% set route_form = 'fis_emissaonfe_form' %}
{% set route_list = 'fis_emissaonfe_list' %}
{% set route_list_ajax = 'fis_emissaonfe_datatablesJsList' %}
{% set list_id = 'emissaoNFeList' %}

{% set campos_pesquisar %}

    <div class="form-group">
        <label for="p_descricao">Destinatário</label>
        <input type="text"
               class="form-control" id="filter[pessoaDestinatario_nome]"
               name="filter[pessoaDestinatario_nome]"
               value="{{ filter.pessoaDestinatario_nome|default('') }}">
    </div>

    <div class="form-group">
        <label for="p_descricao">Dt Emissão</label>
        <input type="date"
               size="15" class="form-control" name="filter[dtEmissao][i]"
               value="{{ filter.dtEmissao.i|default('') }}">
        <input type="date" size="15" class="form-control"
               name="filter[dtEmissao][f]"
               value="{{ filter.dtEmissao.f|default('') }}">
    </div>

{% endset %}


{% block down_scripts %}
    <script src="{{ asset('build/fis/emissaoNFe/list.js') }}"></script>
{% endblock %}

